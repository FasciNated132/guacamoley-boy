var loadIndex = 0;
load = function() {
   switch(loadIndex) {
        case 0:
            drawHomeMap();
            loadedBackgroundPics.drawHomeMap = get(0, 0, width, height);
        break;    
       case 1:
           background1();
           loadedBackgroundPics.background1 = get(0, 0, width, height);
           break;
