    fill(255, 0, 0);
    noStroke();
    arc(215, 350, 5, 20, 180, 360);
    //}
    
    //overlapping black stone
   fill(0);
   triangle(110, 345, 125, 345, 108, 325);
    
    
    drawCastleMap(150, 300, city1);
    
    pushMatrix();
    translate(20, 350);
    rotate(20);
    translate(-20, -380);
    drawCastleMap(20, 350, city2);
