            pushMatrix();    
               
            if(cameraView) {
                translate(translateX, translateY);   
            }    
             
             
           runObjects();
            
            player1[0].draw();   
             
            if(keys[82]) {
                resetLevel();
            }    
           
            if(player1[0].x >= levelW-blockSize) {
                gamePage = 30;
        
            } 
          popMatrix(); 
           pauseButton();
           
            backButton();
      
            levelPickerButton();
        break;  
        case 30:
            image(loadedBackgroundPics.background14, 0, 0, width, height);
            Text(10, 20, 0.75, "I`m being driven straight ", 2);
            Text(10, 50, 0.7, "into the volcano.", 2); 
