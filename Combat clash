            if(keys[82]) {
                resetLevel();
            }    
           
            if(player1[0].hasPearl === true) {
                gamePage = 40;
            } 
          popMatrix(); 
           pauseButton();
           
      backButton();
    
      levelPickerButton();
        break; 
         case 40:
              background(0, 0, 0);
             pearl(300, 300, 4);
             
