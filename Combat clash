    vertex(90, 390);
    vertex(130, 380);
    vertex(125, 370);
    vertex(123, 380);
    vertex(118, 375);
    vertex(110, 360);
    vertex(100, 370);
    vertex(95, 372);
    vertex(85, 350);
    endShape();
};  
var finalBackground = function() {
    background(102, 102, 102);



//wall{
  for(var i = 0; i < width; i += 40) {
      for(var y = 0; y < 300; y += 44) {
            fill(random(150, 200));
            noStroke();
            rect(i, y, 38, 20, 5);
      }
  }  
  for(var i = -20; i < width; i += 40) {
      for(var y = -22; y < 300; y += 44) {
            fill(random(150, 200));
            noStroke();
            rect(i, y, 38, 20, 5);
      }
  }  
//}

//pillars{
var Pillar = function(x) {
    pushMatrix();
    translate(x-100, 0);
    fill(207, 177, 41);
    rect(100, 0, 50, 380);
    ellipse(125, 380, 50, 15);
    strokeWeight(5);
    stroke(224, 199, 98);
    line(145, 0, 145, 379);
    stroke(209, 183, 77);
    line(140, 0, 140, 381);
    stroke(196, 169, 61);
    line(133, 0, 133, 382);
    stroke(184, 159, 57);
    line(124, 0, 124, 383);
    stroke(168, 143, 42);
    line(116, 0, 116, 382);
    stroke(150, 126, 30);
    line(110, 0, 110, 381);
    stroke(150, 126, 30);
    line(103, 0, 103, 379);
    noStroke();
    popMatrix();
};
Pillar(50);
